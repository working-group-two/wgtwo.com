{
  "name": "working-group-two-website",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "fetch-personio-jobs": "curl https://wgtwo.jobs.personio.de/xml -o personio-jobs.xml",
    "links:check": "linkinator -r http://localhost:3000",
    "links:check:json": "linkinator -r --format JSON http://localhost:3000 | jq '.links | .[] | select(.state | contains(\"BROKEN\"))'",
    "links:test": "start-server-and-test serve 3000 links:check"
  },
  "dependencies": {
    "@cmfcmf/docusaurus-search-local": "^0.10.0",
    "@docusaurus/core": "^2.1.0",
    "@docusaurus/plugin-client-redirects": "^2.1.0",
    "@docusaurus/plugin-ideal-image": "^2.1.0",
    "@docusaurus/preset-classic": "^2.1.0",
    "@img-comparison-slider/react": "^7.4.3",
    "@types/node-fetch": "^3.0.3",
    "classnames": "^2.2.6",
    "img-comparison-slider": "^7.4.3",
    "isomorphic-fetch": "^3.0.0",
    "jsdom": "^19.0.0",
    "linkinator": "^4.0.3",
    "lucide-react": "^0.16.25",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-photo-gallery": "^8.0.0",
    "start-server-and-test": "^1.14.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "file-loader": "^6.2.0",
    "prettier": "2.5.1"
  },
  "prettier": {
    "semi": false,
    "arrowParens": "avoid"
  }
